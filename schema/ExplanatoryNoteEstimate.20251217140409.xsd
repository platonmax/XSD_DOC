        <xs:annotation>
            <xs:documentation xml:lang="ru">Перечень ресурсов</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Resource" type="tResource" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Сведения о необходимом ресурсе (сырье)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tBudgetSource">
