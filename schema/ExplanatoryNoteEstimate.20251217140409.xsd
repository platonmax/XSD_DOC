<?xml version="1.0" encoding="utf-8" ?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" version="1.01"
    xmlns:xs="http://www.w3.org/2001/XMLSchema" vc:minVersion="1.1"
    xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning">

    <xs:annotation>
        <xs:documentation xml:lang="ru">Корневой элемент Пояснительной записки.
Элемент обязан содержать атрибуты, указывающие на применяемый xsd-шаблон описания пояснительной записки:
xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance";
xsi:noNamespaceSchemaLocation = "ExplanatoryNoteEstimate-4_01.xsd".
Электронные документы, представляемые для проведения государственной экспертизы проектной документации и (или) результатов инженерных изысканий и проверки достоверности определения сметной стоимости строительства, реконструкции, капитального ремонта, должны иметь расширение файла .gge</xs:documentation>
    </xs:annotation>
    <xs:complexType name="tForeignOrganization">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Иностранное юридическое лицо (филиал, представительство)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="OrgFullName" type="non-empty-string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Полное наименование иностранного юридического лица (филиала, представительства)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OrgINN" type="tOrgINN" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Индивидуальный номер налогоплательщика – иностранного юридического лица (филиала, представительства)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OrgKPP" type="tOrgKPP" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Код причины постановки на учет иностранного юридического лица (филиала, представительства)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Address" type="tAddress" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Адрес (местонахождение) филиала или представительства иностранного юридического лица в Российской Федерации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Email" type="tEmail" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Адрес электронной почты иностранного юридического лица (филиала, представительства)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="Person" type="tPerson">
                <xs:annotation>
                    <xs:documentation>Сведения о физическом лице</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tSRO">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Сведения о саморегулируемой организации</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="non-empty-string">
                <xs:attribute name="SROType" type="tSROType" use="required">
                    <xs:annotation>
                        <xs:documentation xml:lang="ru">Тип саморегулируемой организации</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="tOrgINN">
        <xs:annotation>
            <xs:documentation xml:lang="ru">ИНН организации</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:length value="10"/>
            <xs:pattern value="\d{10}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tOrgKPP">
        <xs:annotation>
            <xs:documentation xml:lang="ru">КПП организации</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:length value="9"/>
            <xs:pattern value="\d{9}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tEmail">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка - адрес электронной почты</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:whiteSpace value="replace"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tSROType">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Виды саморегулируемых организаций в строительной области</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Саморегулируемая организация в области инженерных изысканий"/>
            <xs:enumeration
                value="Саморегулируемая организация в области архитектурно-строительного проектирования"/>
            <xs:enumeration value="Саморегулируемая организация в области строительства"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="string200">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка длиной до 200 символов</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:maxLength value="200"/>
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="string500">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Текстовая строка длиной до 500 символов</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:normalizedString">
            <xs:maxLength value="500"/>
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tRegionsRF">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Коды субъектов Российской Федерации (от 00 до 99)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{2}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="non-empty-string">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Непустая текстовая строка</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="(\s*[^\s]\s*)+"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tGUID">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Уникальный идентификатор GUID</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern
                value="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}"
            />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tCalculationMethod">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Метод определения сметной стоимости</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Ресурсно-индексный"/>
            <xs:enumeration value="Базисно-индексный"/>
            <xs:enumeration value="Ресурсный"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tBudgetLevel">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Уровень бюджета</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Федеральный бюджет"/>
            <xs:enumeration value="Бюджет субъекта Российской Федерации"/>
            <xs:enumeration value="Местный бюджет"/>
            <xs:enumeration value="Бюджет государственного внебюджетного фонда Российской Федерации"/>
            <xs:enumeration value="Бюджет территориального государственного внебюджетного фонда"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tConstructionType">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Вид работ, на который разрабатывается проектная документация:
1 - Строительство;
2 - Реконструкция;
3 - Капитальный ремонт;
4 - Снос;
5 - Сохранение объектов культурного наследия.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Строительство</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Реконструкция</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Капитальный ремонт</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Снос</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="5">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Сохранение объекта культурного наследия</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tPositiveDecimal">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Положительное десятичное число</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tFireDangerCategory">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Категория помещений, зданий и наружных установок по взрывопожарной и пожарной опасности</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="А"/>
            <xs:enumeration value="Б"/>
            <xs:enumeration value="В"/>
            <xs:enumeration value="Г"/>
            <xs:enumeration value="Д"/>
            <xs:enumeration value="Категория не устанавливается"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tResponsibilityLevel">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Уровень ответственности здания или сооружения</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="повышенный"/>
            <xs:enumeration value="нормальный"/>
            <xs:enumeration value="пониженный"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tFunctionsClass">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Код классификатора по функциональному назначению</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern
                value="([0-9]{1,2}\.[0-9]{1,2}\.[0-9]{1,3}\.[0-9]{1,3})|([0-9]{1,2}\.[0-9]{1,2}\.[0-9]{1,3})"
            />
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="TMeta">
        <xs:annotation>
            <xs:documentation>Информация о программном комплексе и версии формата</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Soft">
                <xs:annotation>
                    <xs:documentation>Программный комплекс</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Name" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Наименование программного комплекса</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Version" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Версия программного комплекса</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="File" type="TFileFormat">
                <xs:annotation>
                    <xs:documentation>Тип и версия передаваемого файла</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tTEIBase">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Базовый тип для технико-экономических показателей, содержащий общие поля</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="non-empty-string">
                <xs:annotation>
                    <xs:documentation>Наименование показателя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Measure" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Единица измерения показателя (кодовое буквенное национальное обозначение по ОКЕИ), либо «-» в случае невозможности указать единицу измерения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Value" type="non-empty-string">
                <xs:annotation>
                    <xs:documentation>Значение показателя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OldValue" type="non-empty-string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Предыдущее значение показателя (при реконструкции)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tMainTEI">
        <xs:annotation>
            <xs:documentation>Главный технико-экономический показатель (все поля комплексного типа обязательны для заполнения)</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tTEIBase">
                <xs:sequence>
                    <xs:element name="UnitCost" type="xs:decimal" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Стоимость за единицу показателя (обязательное поле)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="tAdditionalTEI">
        <xs:annotation>
            <xs:documentation>Технико-экономический показатель</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tTEIBase">
                <xs:sequence>
                    <xs:element name="UnitCost" type="xs:decimal" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Стоимость за единицу показателя</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="tCoordinates">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Тип для координат стройки в WGS 84 ((World Geodetic System 1984) — глобальная геодезическая система координат)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Latitude" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Широта (latitude) в десятичных градусах (диапазон: -90 до 90). Обязательное поле внутри координат</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Longitude" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Долгота (longitude) в десятичных градусах (диапазон: -180 до 180). Обязательное поле внутри координат</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tAddress">
        <xs:annotation>
            <xs:documentation>Адрес</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tAddressBase"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="tTextBlock">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Дополнительное произвольное текстовое описание
</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Text" type="non-empty-string" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Дополнительное произвольное текстовое описание
</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TPriceLevel">
        <xs:annotation>
            <xs:documentation>Описывает уровень цен, в котором составлен документ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Year">
                <xs:annotation>
                    <xs:documentation>Год</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedShort">
                        <xs:minInclusive value="1964"/>
                        <xs:maxInclusive value="2100"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Month" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Месяц</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedByte">
                        <xs:minInclusive value="1"/>
                        <xs:maxInclusive value="12"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Quarter">
                <xs:annotation>
                    <xs:documentation>Квартал</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedByte">
                        <xs:minInclusive value="1"/>
                        <xs:maxInclusive value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Day" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>День</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedByte">
                        <xs:minInclusive value="1"/>
                        <xs:maxInclusive value="31"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="CalculationMethod" type="tCalculationMethod" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Метод определения сметной стоимости</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Note" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Примечание</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tOrganization">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Описание организации - юридического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="FullName" type="non-empty-string">
                <xs:annotation>
                    <xs:documentation>Полное наименование юридического лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="INN" type="non-empty-string">
                <xs:annotation>
                    <xs:documentation>Индивидуальный номер налогоплательщика - юридического лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OGRN" type="non-empty-string">
                <xs:annotation>
                    <xs:documentation>Основной государственный регистрационный номер юридического лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Address" type="tAddress">
                <xs:annotation>
                    <xs:documentation>Адрес (местонахождение) юридического лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Email" type="tEmail" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Адрес электронной почты иностранного юридического лица (филиала, представительства)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="Person" type="tPerson">
                <xs:annotation>
                    <xs:documentation>Сведения о физическом лице</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TFileFormat">
        <xs:annotation>
            <xs:documentation>Комплексный тип для описания типа и версии передаваемого файла</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Type">
                <xs:annotation>
                    <xs:documentation>Тип передаваемого файла: ПЗ - Пояснительная записка</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="ПЗ"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Version" default="4.01">
                <xs:annotation>
                    <xs:documentation>Номер версии схемы</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:fractionDigits value="2"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tPerson">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Описание физического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="FamilyName" type="non-empty-string">
                <xs:annotation>
                    <xs:documentation>Фамилия</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FirstName" type="non-empty-string">
                <xs:annotation>
                    <xs:documentation>Имя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SecondName" type="non-empty-string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Отчество</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tFile">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Описание файла, включая его имя, формат и контрольную сумму для проверки целостности</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="FileName" type="non-empty-string">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Имя файла с расширением. Имя файла должно быть уникальным в рамках одного документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FileFormat" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Формат файла (например: gge, pdf, xlsx, docx и т.д.)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FileChecksum">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Контрольная сумма файла по алгоритму CRC32-IEEE для проверки целостности при передаче. Расчет контрольной суммы файла должен быть выполнен без элемента ExportDateTime</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="[0-9a-fA-F]{32}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tTextBlockWithLinks" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Произвольное текстовое описание (поддерживается вставка ссылки на документ, который был добавлен в ПЗ (элемент ProjectDocumentationContent), через его GUID)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="GUID" type="tGUID" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">
                        Уникальный идентификатор документа, на который делается ссылка
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TSignatures">
        <xs:annotation>
            <xs:documentation>Подписи пояснительной записки</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ComposeFIO" type="TExtSignature" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>Cоставил (ФИО) (Подпись исполнителя)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="VerifyFIO" type="TExtSignature" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>Проверил (ФИО) (Подпись представителя заказчика)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TExtSignature">
        <xs:annotation>
            <xs:documentation>Подпись (Заказчик)</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="TSignature">
                <xs:sequence>
                    <xs:element name="Position" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Должность</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="RegistryNumber" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Номер в реестре</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TSignature">
        <xs:annotation>
            <xs:documentation>Подпись в пояснительной записке</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name">
                <xs:annotation>
                    <xs:documentation>Значение подписи</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tConstructionDates">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Даты, связанные со строительством</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="BeginDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата начала работ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EndDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата окончания работ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OperationDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата ввода в эксплуатацию</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tFunctions">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Функциональное назначение объекта
</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="FunctionsNote" type="non-empty-string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Функциональное назначение объекта капитального строительства
</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FunctionsClass" type="tFunctionsClass">
                <xs:annotation>
                    <xs:documentation>Функциональное назначение объекта капитального строительства (код по классификатору объектов капитального строительства по их назначению и функционально-технологическим особенностям)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tObjectParts">
        <xs:annotation>
            <xs:documentation>Комбинированный тип, описывающий составную часть объекта (здание, сооружение) и содержащий список его технико-экономических показателей</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ObjectPart" type="tBuildingDataTEI" minOccurs="1"
                maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Комбинированный тип, описывающий составную часть объекта (здание, сооружение) и содержащий список его технико-экономических показателей</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:attribute name="ElectronicBudgetObjectCode" type="tElectronicBudgetObjectCode">
        <xs:annotation>
            <xs:documentation>Уникальный код объекта капитального строительства, присвоенный государственной интегрированной информационной системой управления общественными финансами "Электронный бюджет"</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:complexType name="tAddressBase">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Базовый тип адреса, содержащий общие поля</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Country" type="string500" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Страна</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Region" type="tRegionsRF" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="ru">Территориальное расположение объекта строительства. Код региона (при отсутствии кода субъекта заполняется "00")</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="District" type="string200" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Область</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="City" type="string200" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Город</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Settlement" type="string200" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Поселок</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Street" type="string200" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Улица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Building" type="string200" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Номер здания (номер дома)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Room" type="string200" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Номер помещения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Coordinates" type="tCoordinates" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Тип для координат стройки в WGS 84 ((World Geodetic System 1984) — глобальная геодезическая система координат)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Note" type="non-empty-string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Неформализованное описание адреса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PriceZone" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Подрайон, ценовая зона в составе основного региона (субъект Российской Федерации), в котором расположен объект строительства</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="SubRegionCode" type="string200">
                            <xs:annotation>
                                <xs:documentation>Код подрайона, ценовой зоны (при отсутствии кода заполняется "00")</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="SubRegionName" type="string200">
                            <xs:annotation>
                                <xs:documentation>Наименование подрайона, ценовой зоны (Зона)</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tObjectAddress">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Тип адреса для объекта строительства. Включает обязательный признак территории</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tAddressBase">
                <xs:sequence>
                    <xs:element name="TerritorySign" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation xml:lang="ru">Обозначение территории расположения объекта строительства:
- не относящейся к районам Крайнего Севера и местностям, приравненным к ним (Территория);
- относящейся к местностям, приравненным к районам Крайнего Севера (МПРКС);
- относящейся к районам Крайнего Севера (РКС);
- другое.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="string200">
                                <xs:enumeration value="территория"/>
                                <xs:enumeration value="РКС"/>
                                <xs:enumeration value="МПРКС"/>
                                <xs:enumeration value="Другое"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="tResource">
        <xs:annotation>
            <xs:documentation xml:lang="ru">Описание одного ресурса (энергетического, материального и т.д.)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="non-empty-string">
                <xs:annotation>
                    <xs:documentation>Наименование необходимого ресурса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Measure" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Единица измерения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Value" type="non-empty-string">
                <xs:annotation>
                    <xs:documentation>Необходимый объем ресурса</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
